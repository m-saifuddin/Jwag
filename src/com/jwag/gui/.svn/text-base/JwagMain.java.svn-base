    private void btn_disconnectActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btn_disconnectActionPerformed
        
        if (!btn_DBconnect.isEnabled()) {
            setTextFieldsEditable(true);
            
            try {
                
                listModel.clear();
                combo_dbtables.removeAllItems();
                DBConnection.closeConnection(DBConnection.connObject);
                tableName = null;
                columnNames = null;
            } catch (Exception ex) {
                
                System.out.println("Exception on Disconnect :" + ex.toString());
            } //catch
        } //if
    }//GEN-LAST:event_btn_disconnectActionPerformed
    
    private void combo_dbtablesItemStateChanged(java.awt.event.ItemEvent evt) {//GEN-FIRST:event_combo_dbtablesItemStateChanged
        
        if (comboBoxModel.getSize() > 0) {
            
            tableName = combo_dbtables.getSelectedItem().toString();
            
            dbmetadata = new com.jwag.db.DatabaseMetaData();
            
            listModel = new DefaultListModel();
            list_all_columns.setModel(listModel);
            
            try {
                
                List<String> column_metadata = dbmetadata.getColumnsName(tableName);
                
                for (String columName : column_metadata) {
                    
                    listModel.addElement(columName);
                }
            } catch (SQLException sqlex) {
                
                System.out.println("Exception Occur in Loading Columns : " + sqlex.getMessage());
            }
            try {
                boolean value = dbmetadata.hasPrimaryKey(tableName);
                System.out.println("boolean value " + value);
                
                if (value) {
                    System.out.println("boolean value " + value);
                    btnGenerator.setEnabled(true);
                } else {
                    btnGenerator.setEnabled(false);
                }
            } catch (SQLException ex) {
                
                System.out.println("Exception Occur in Primary Key Checking : " + ex.getMessage());
            }
        } // if
        list2Model = new DefaultListModel();
        list_selected_columns.setModel(list2Model);
    }//GEN-LAST:event_combo_dbtablesItemStateChanged
    
    private void btn_DBconnectActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btn_DBconnectActionPerformed
        
        comboBoxModel = new DefaultComboBoxModel();
        combo_dbtables.setModel(comboBoxModel);
        
        /**********Filling Bean***********/
        connDB = ConnectDB.getIntance();
        connDB.setLocalhost(txt_hostname.getText().toString().trim());
        connDB.setDbname(txt_dbname.getText().toString().trim());
        connDB.setDbport(txt_dbport.getText().toString().trim());
        connDB.setDbusrname(txt_dbusername.getText().toString().trim());
        connDB.setDbpaswrd(txt_dbpassword.getText());
        
        dsDataHolder = new DataSourceDataHolder();
        dsDataHolder.setDatabaseName(connDB.getDbname());
        dsDataHolder.setHostName(connDB.getLocalhost());
        dsDataHolder.setDriverClassName("com.mysql.jdbc.Driver");
        dsDataHolder.setUrl("jdbc:mysql://" + connDB.getLocalhost() + "/" + connDB.getDbname());
        dsDataHolder.setUsername(connDB.getDbusrname());
        dsDataHolder.setPassword(connDB.getDbpaswrd());
        
        dbmetadata = new com.jwag.db.DatabaseMetaData();
        
        try {
            
            List<String> table = dbmetadata.getTablesName();
            int j = 0;
            
            for (String tablename : table) {
                
                comboBoxModel.addElement(tablename);
            }
            
            btn_DBconnect.setEnabled(false);
            jTAMessages.setText("Connecting... Connection Build Successfully");
            
        } catch (Exception ex) {
            JOptionPane.showMessageDialog(null, "Provided Connection Details is Wrong \n Check your Details","Error",JOptionPane.INFORMATION_MESSAGE);
            jTAMessages.setText("");
            System.out.println("Exception in Loading Table : " + ex.getMessage() + " " + " " + ex.getCause());
            
        }
    }//GEN-LAST:event_btn_DBconnectActionPerformed
    
