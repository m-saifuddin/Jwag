    private void btnGeneratorActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnGeneratorActionPerformed
        
        columnNames = new ArrayList<String>();
        projectPackage = txt_packagename.getText().trim();
        applicationName = txtAppName.getText().trim();
        
        if (list_selected_columns.getModel().getSize() > 0) {
            
            for (int i = 0; i < list_selected_columns.getModel().getSize(); i++) {
                
                columnNames.add(list_selected_columns.getModel().getElementAt(i).toString());
            }
            if (columnNames.contains("id")) {
            } else {
                
                for (int j = 0; j < list_all_columns.getModel().getSize(); j++) {
                    
                    if (list_all_columns.getModel().getElementAt(j).toString().equalsIgnoreCase("id")) {
                        
                        columnNames.add(list_all_columns.getModel().getElementAt(j).toString());
                        System.out.println("id added into columnNames");
                    }
                }
            }
            
            ArrayList<String> jspnames = new ArrayList<String>();
            ArrayList<String> daonames = new ArrayList<String>();
            ArrayList<String> controlname = new ArrayList<String>();
            
            
            
            try {
                
                DaoGenerator daoGen = new DaoGenerator();
                JspGenerator jspGen = new JspGenerator();
                BeanGenerator beanGen = new BeanGenerator();
                WebXmlGenerator webxmlGen = new WebXmlGenerator();
                ControllerGenerator controllerGen = new ControllerGenerator();
                IndexJspGenerator indexJspGen = new IndexJspGenerator();
                applicationContextGen = new ApplicationContextGenerator();
                
                /****** Generating a bean ****/
                String beanName = beanGen.generate(tableName, projectPackage);
                System.out.println("bean generated");
                
                jTAMessages.append("\n*************************************\n");
                jTAMessages.append("Bean File is Generated\n");
                jTAMessages.append("*************************************\n");
                /****************************************/
                
                /********* Generating WebXml file *****/
                webxmlGen.generate();
                jTAMessages.append("\n*************************************\n");
                jTAMessages.append("web.xml is Generated \n");
                jTAMessages.append("*************************************\n");
                
                System.out.println("webxml Generated");
                /****************************************/
                
                /****** Generating a jsp files ****/
                jspnames = jspGen.generate(beanName, tableName);
                jTAMessages.append("\n*************************************\n");
                jTAMessages.append("Generating Java Server Pages \n");
                jTAMessages.append("*************************************\n");
                for(String jsp : jspnames){
                    
                    jTAMessages.append("\n "+jsp+".jsp \n");
                    
                }
                //jTAMessages.append("*************************************\n");
                /****************************************/
                
                /****** Generating a Dao files ****/
                daonames = daoGen.generate(tableName, projectPackage);
                System.out.println("daos Generated");
                jTAMessages.append("\n*************************************\n");
                jTAMessages.append("DAO Stuff Generated \n");
                jTAMessages.append("*************************************\n");
                
                
                for(String dao : daonames){
                    
                    jTAMessages.append(" "+dao+".java \n");
                }
                //jTAMessages.append("*************************************\n");
                /****************************************/
                
                /****** Generating a jsp files ****/
                controlname = controllerGen.generate(tableName, jspnames, daonames, projectPackage);
                System.out.println("Controller Generated");
                
                 jTAMessages.append("\n*************************************\n");
                jTAMessages.append("Controller Generated \n");
                jTAMessages.append("*************************************\n");
                
                for(String controller : controlname){
                    
                    jTAMessages.append(" "+controller+".java \n");
                }
                //jTAMessages.append("_____________________________________\n");
                /****************************************/
                
                
                /********* Generating ApplicationContext *****/
                applicationContextGen.generate(tableName, jspnames, dsDataHolder, daonames, controlname, projectPackage);
                
                jTAMessages.append("\n*************************************\n");
                jTAMessages.append("Spring Application Context Generated \n");
                jTAMessages.append("*************************************\n");
                
                
                /****************************************/
                
                /********* Generating index.jsp *****/
                indexJspGen.generate(tableName);
                jTAMessages.append("\n*************************************\n");
                jTAMessages.append("index.jsp Generated \n");
                jTAMessages.append("*************************************\n");
                /****************************************/
                JOptionPane.showMessageDialog(null, "All Files are Generated Successfully", "Information", JOptionPane.INFORMATION_MESSAGE);
            } catch (Exception ex) {
                JOptionPane.showMessageDialog(null, " "+ex.getStackTrace(), "Alert", JOptionPane.ERROR_MESSAGE);
                
                System.out.println("Exception in Calling GenerateClass Method : " + ex.getMessage());
            }
        } else {
            
            JOptionPane.showMessageDialog(null, "You haven't selected Column for List View Page", "Alert", JOptionPane.ERROR_MESSAGE);
        }
    }//GEN-LAST:event_btnGeneratorActionPerformed
    
