    private void btn_removeActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btn_removeActionPerformed
        
        if (!list_selected_columns.isSelectionEmpty()) {
            
            String selected_field = "";
            
            selected_field = String.valueOf(list_selected_columns.getSelectedValue());
            
            list2Model.remove(list_selected_columns.getSelectedIndex());
            
            if (listModel.size() == 0) {
                
                listModel.addElement(selected_field);
            } else {
                
                int indexsize = listModel.getSize();
                listModel.add(indexsize++, selected_field);
            } //else
        } else {
            
            JOptionPane.showMessageDialog(null, "You haven't select the field yet", "Alert", JOptionPane.INFORMATION_MESSAGE);
        } //else
    }//GEN-LAST:event_btn_removeActionPerformed
    
    private void btn_removeallActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btn_removeallActionPerformed
        
        ArrayList arr_selected_col = new ArrayList();
        
        if (list_selected_columns.getModel().getSize() != 0) {
            
            for (int i = 0; i < list2Model.getSize(); i++) {
                
                arr_selected_col.add(list2Model.getElementAt(i));
            } // for
            list2Model.removeAllElements();
            
            for (int j = 0; j < arr_selected_col.size(); j++) {
                
                listModel.add(j, arr_selected_col.get(j));
            } //for
        } else {
            
            JOptionPane.showMessageDialog(null, "Selected Fields List is Empty", "Alert", JOptionPane.INFORMATION_MESSAGE);
        } //else
    }//GEN-LAST:event_btn_removeallActionPerformed
    
